  tls whatever4711@gmail.com
  gzip 
  log / /root/logs/cloud.log "{proto} Request: {method} {path} ... {scheme} {host} {remote}"
}
  proxy / cloud_cloud_1: {
    proxy_header Host {host}
    proxy_header X-Real-IP {remote}
    proxy_header X-Forwarded-Proto {scheme}
  }
}
